{
  "name": "@esoto/helpers",
  "version": "0.0.1-rc.2",
  "description": "esoto helpers utilities",
  "author": "Eric Sotomayor <elpasocolorartist@gmail.com>",
  "license": "ISC",
  "main": "./dist/esoto-helpers.cjs.js",
  "module": "./dist/esoto-helpers.es.js",
  "typings": "./dist/esoto-helpers.d.js",
  "files": [
    "./dist/**/*",
    "dist/**/*.js",
    "dist/*.d.ts",
    "./dist/scripts/**/*"
  ],
  "exports": {
    ".": {
      "require": "./dist/esoto-helpers.cjs.js",
      "import": "./dist/esoto-helpers.es.js",
      "types": "./dist/esoto-helpers.d.ts"
    },
    "./core": {
      "require": "./dist/shared/core.cjs.js",
      "import": "./dist/shared/core.es.js",
      "types": "./dist/shared/core.d.ts"
    },
    "./node": {
      "require": "./dist/shared/core.cjs.js",
      "import": "./dist/shared/node.es.js",
      "types": "./dist/shared/node.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -cw"
  },
  "dependencies": {
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  },
  "devDependencies": {
    "@types/node": "^18.7.14"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esoto76/esoto.git"
  },
  "bugs": {
    "url": "https://github.com/esoto76/esoto/issues"
  },
  "homepage": "https://github.com/esoto76/esoto/tree/main/packages/helpers#readme"
}